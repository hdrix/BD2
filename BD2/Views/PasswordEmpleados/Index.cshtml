@model IEnumerable<BD2.Model.PasswordEmpleados>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.enable)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.fecha_Modificado)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.Empleado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Passwords)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.enable)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fecha_Modificado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Empleado.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Passwords.ID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
